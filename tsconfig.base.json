{
  "compilerOptions": {
    "target": "ES2022",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "types": []
  },
  "exclude": ["node_modules", "dist", "build", "cdk.out"]
}
